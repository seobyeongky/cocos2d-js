 #include <vector>
 using namespace std;
 
+string getProjSearchPath()
+{
+    NSString *bundlePath = [[NSBundle mainBundle] resourcePath];
+    if (bundlePath != nil) {
+        return [bundlePath UTF8String];
+    }
+    return "";
+}
