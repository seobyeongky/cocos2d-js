+{
+    lazyInit();
+    return s_visibleRect.origin;
+}
+
+Point VisibleRect::rightBottom()
+{
+    lazyInit();
+    return Point(s_visibleRect.origin.x+s_visibleRect.size.width, s_visibleRect.origin.y);
